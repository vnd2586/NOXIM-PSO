#Scenario 2
#T: Throughput
set term postscript eps color blacktext "Throughput" 24
set out "T_s2.eps"
set title "Throughput (Flits/Cycle/IP)"
set xlabel "Packet Size (Flits)"
#set xrange [0:10]
set ylabel "Throughpt (Flits/Cycle/IP)"
set key bottom right
plot "dataout_s2_bf_date070416" using 1:6 title "before PSO" lw 16 lc rgb "blue", \
"dataout_s2_af_date070416" using 1:6 title "after PSO" lw 10 lc rgb "red"
set output
quit


